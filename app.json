{
  "expo": {
    "name": "18火把特會",
    "description": "A very interesting project.",
    "slug": "tls18torchchurch",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#0E2547"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E2547"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tls18torchchurch",
      "supportsTablet": true,
      "icon": "./assets/images/iconIos.png",
      "infoPlist": { "NSLocationWhenInUseUsageDescription": "We didn't use your location. It's deafult feature from expo." }
    },
    "android": {
      "package": "com.torchchurch18tls",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/images/icon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#C0C0C0"
    }
  }
}
